    <script>
        // Initialize GSAP
        gsap.registerPlugin(ScrollTrigger);
        
    
        
        // Handle section animations
        const sections = document.querySelectorAll('.section-hidden');
        
        sections.forEach(section => {
            gsap.to(section, {
                opacity: 1,
                y: 0,
                duration: 1,
                scrollTrigger: {
                    trigger: section,
                    start: 'top 85%',
                    toggleActions: 'play none none none'
                }
            });
        });
        
        // Add hover effects
        document.querySelectorAll('.bg-white, .bg-gray-50').forEach(item => {
            item.addEventListener('mouseenter', () => {
                item.classList.add('shadow-xl');
                item.classList.remove('shadow-lg');
            });
            item.addEventListener('mouseleave', () => {
                item.classList.remove('shadow-xl');
                item.classList.add('shadow-lg');
            });
        });
    </script>
